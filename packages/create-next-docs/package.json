{
  "name": "create-next-docs-app",
  "description": "Create a new documentation site with Next Docs UI",
  "version": "1.5.10",
  "homepage": "https://next-docs-zeta.vercel.app",
  "author": "Money SonMooSans",
  "repository": "github:SonMooSans/next-docs",
  "license": "MIT",
  "files": [
    "templates/*",
    "dist/*"
  ],
  "main": "./dist/index.js",
  "bin": "./dist/index.js",
  "scripts": {
