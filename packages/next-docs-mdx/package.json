{
  "name": "next-docs-mdx",
  "description": "The library for using native Next.js MDX features in Next Docs",
  "version": "4.0.9",
  "homepage": "https://next-docs-zeta.vercel.app",
  "author": "Fuma Nama",
  "repository": "github:fuma-nama/next-docs",
  "license": "MIT",
  "files": [
    "dist/*",
    "loader.js"
  ],
  "exports": {
    "./loader": "./loader.js",
    "./*": {
      "import": "./dist/*.mjs",
      "types": "./dist/*.d.mts"
    }
  },
  "typesVersions": {
    "*": {
      "config": [
        "./dist/config.d.mts"
      ],
      "map": [
        "./dist/map.d.mts"
      ],
      "types": [
        "./dist/types.d.mts"
      ]
    }
  },
  "scripts": {
    "build": "tsup",
    "clean": "rmdir /q/s dist",
    "dev": "tsup --watch",
    "prepublishOnly": "pnpm build",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "estree-util-value-to-estree": "^3.0.1",
    "fast-glob": "^3.3.1",
    "remark-frontmatter": "^5.0.0",
    "yaml": "^2.3.3",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "@next/mdx": ">= 13",
    "next-docs-zeta": "4.0.9"
  },
  "devDependencies": {
    "@next/mdx": "14.0.3",
    "@types/mdast": "^3.0.12",
    "@types/mdx": "^2.0.8",
    "@types/react": "18.2.0",
    "next": "14.0.3",
    "next-docs-zeta": "workspace:*",
    "unified": "^10.1.2",
    "webpack": "^5.89.0"
  },
  "keywords": [
    "NextJs",
    "Docs"
  ],
  "publishConfig": {
    "access": "public"
  }
}
