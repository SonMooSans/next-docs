{
  "name": "next-docs-zeta",
  "description": "The library for building a documentation website in Next.js",
  "version": "4.0.9",
  "homepage": "https://next-docs-zeta.vercel.app",
  "author": "Fuma Nama",
  "repository": "github:fuma-nama/next-docs",
  "type": "module",
  "license": "MIT",
  "files": [
    "dist/*"
  ],
  "exports": {
    "./sidebar": {
      "import": "./dist/sidebar.js",
      "types": "./dist/sidebar.d.ts"
    },
    "./breadcrumb": {
      "import": "./dist/breadcrumb.js",
      "types": "./dist/breadcrumb.d.ts"
    },
    "./toc": {
      "import": "./dist/toc.js",
      "types": "./dist/toc.d.ts"
    },
    "./search/client": {
      "import": "./dist/search/client.js",
      "types": "./dist/search/client.d.ts"
    },
    "./search/server": {
      "import": "./dist/search/server.js",
      "types": "./dist/search/server.d.ts"
    },
    "./server": {
      "import": "./dist/server.js",
      "types": "./dist/server.d.ts"
    },
    "./link": {
      "import": "./dist/link.js",
      "types": "./dist/link.d.ts"
    },
    "./contentlayer": {
      "import": "./dist/contentlayer/index.js",
      "types": "./dist/contentlayer/index.d.ts"
    },
    "./contentlayer/configuration": {
      "import": "./dist/contentlayer/configuration.js",
      "types": "./dist/contentlayer/configuration.d.ts"
    },
    "./middleware": {
      "import": "./dist/middleware.js",
      "types": "./dist/middleware.d.ts"
    },
    "./mdx-plugins": {
      "import": "./dist/mdx-plugins.js",
      "types": "./dist/mdx-plugins.d.ts"
    },
    "./search-algolia/client": {
      "import": "./dist/search-algolia/client.js",
      "types": "./dist/search-algolia/client.d.ts"
    },
    "./search-algolia/server": {
      "import": "./dist/search-algolia/server.js",
      "types": "./dist/search-algolia/server.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "sidebar": [
        "./dist/sidebar.d.ts"
      ],
      "breadcrumb": [
        "./dist/breadcrumb.d.ts"
      ],
      "toc": [
        "./dist/toc.d.ts"
      ],
      "search/client": [
        "./dist/search/client.d.ts"
      ],
      "search/shared": [
        "./dist/search/shared.d.ts"
      ],
      "search/server": [
        "./dist/search/server.d.ts"
      ],
      "server": [
        "./dist/server.d.ts"
      ],
      "link": [
        "./dist/link.d.ts"
      ],
      "contentlayer": [
        "./dist/contentlayer/index.d.ts"
      ],
      "contentlayer/configuration": [
        "./dist/contentlayer/configuration.d.ts"
      ],
      "middleware": [
        "./dist/middleware.d.ts"
      ],
      "mdx-plugins": [
        "./dist/mdx-plugins.d.ts"
      ],
      "search-algolia/client": [
        "./dist/search-algolia/client.d.ts"
      ],
      "search-algolia/server": [
        "./dist/search-algolia/server.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup",
    "clean": "rmdir /q/s dist",
    "dev": "tsup --watch",
    "prepublishOnly": "pnpm build",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.5.0",
    "flexsearch": "0.7.21",
    "github-slugger": "^2.0.0",
    "negotiator": "^0.6.3",
    "react-remove-scroll": "^2.5.6",
    "rehype-img-size": "^1.0.1",
    "rehype-pretty-code": "^0.12.0",
    "remark": "^14.0.3",
    "remark-gfm": "^3.0.1",
    "remark-mdx": "^3.0.0",
    "scroll-into-view-if-needed": "^3.1.0",
    "shiki": "^0.14.5",
    "swr": "^2.2.2",
    "unist-util-visit": "^4.1.2"
  },
  "peerDependencies": {
    "next": ">= 13.4",
    "react": ">= 18",
    "react-dom": ">= 18"
  },
  "devDependencies": {
    "@algolia/client-search": "^4.20.0",
    "@types/flexsearch": "0.7.6",
    "@types/hast": "^2.3.5",
    "@types/mdast": "^3.0.12",
    "@types/negotiator": "^0.6.1",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "algoliasearch": "^4.20.0",
    "contentlayer": "^0.3.4",
    "next": "14.0.3",
    "unified": "^10.1.2",
    "@types/node": "18.17.5"
  },
  "keywords": [
    "NextJs",
    "Docs"
  ],
  "publishConfig": {
    "access": "public"
  }
}
