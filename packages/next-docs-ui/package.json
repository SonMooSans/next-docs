{
  "name": "next-docs-ui",
  "description": "The framework for building a documentation website in Next.js",
  "version": "2.0.1",
  "homepage": "https://next-docs-zeta.vercel.app",
  "author": "Money SonMooSans",
  "repository": "github:SonMooSans/next-docs",
  "license": "MIT",
  "files": [
    "dist/*"
  ],
  "exports": {
    "./style.css": "./dist/style.css",
    "./components": {
      "import": "./dist/components/index.mjs",
      "types": "./dist/components/index.d.ts"
    },
    "./components/*": {
      "import": "./dist/components/*.mjs",
      "types": "./dist/components/*.d.ts"
    },
    "./*": {
      "import": "./dist/*.mjs",
      "types": "./dist/*.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "components": [
        "./dist/components/index.d.ts"
      ],
      "components/*": [
        "./dist/components/*.d.ts"
      ],
      "*": [
        "./dist/*.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup --clean && postcss css/styles.css -o ./dist/style.css",
    "clean": "rmdir /q/s dist",
    "dev": "concurrently \"pnpm dev:layout\" \"pnpm dev:tailwind\"",
    "dev:layout": "tsup --watch",
    "dev:tailwind": "postcss css/styles.css -o ./dist/style.css --watch",
    "prepublishOnly": "pnpm build",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-scroll-area": "^1.0.4",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "lucide-react": "^0.277.0",
    "next-docs-zeta": "workspace:*",
    "next-themes": "^0.2.1",
    "react-medium-image-zoom": "^5.1.8",
    "tailwind-merge": "^1.14.0"
  },
  "peerDependencies": {
    "next": "13.x",
    "react": "18.x",
    "react-dom": "18.x"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "autoprefixer": "10.4.15",
    "contentlayer": "^0.3.4",
    "next": "13.4.19",
    "postcss": "8.4.29",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-lightningcss": "^0.9.0",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.6"
  },
  "keywords": [
    "NextJs",
    "Docs"
  ],
  "publishConfig": {
    "access": "public"
  }
}
